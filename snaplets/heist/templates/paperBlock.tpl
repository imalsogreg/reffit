<a href="/view_article?paperid=${idNum}">
  <div class="pr_paper">
    
    <apply template="score_block"/>
    
    <div class="pr_paper_info">
      <div class="pr_title">
	<strong><paper_title/></strong>
      </div>
      <div class="pr_authors" style="float:left">
	<paper_authors/>
      </div>
      
      <div class="field_tags_block">
	<ul class="field_tags_list">
	  <fieldTags>
	    <li>
	      <span class="label label-info">
		<fieldTag/></span></li>
	    
	  </fieldTags>
	</ul>
      </div>
    </div>
  </div>
</a>
